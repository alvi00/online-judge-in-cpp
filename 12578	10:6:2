#include<bits/stdc++.h>
using namespace std;
#define pi acos(-1.0)
void solve();
int main()
{
    int t;
    cin>>t;
    while(t--)
    {
        solve();
    }

    return 0;
}
void solve()
{
    double l;
    cin>>l;
    double w=l*0.6;
    double ans1=l*w;
    double r=l*0.2;
    double ans2=pi*pow(r,2);
    printf("%0.2lf %0.2lf\n",ans2,ans1-ans2);

}
