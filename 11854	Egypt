#include<bits/stdc++.h>
using namespace std;
int main()
{
    int a,b,c;
    while(1)
    {
         cin>>a>>b>>c;
        if(a==0 && b==0 && c==0)
        {
            break;
        }
        int x=a*a;
        int y=b*b;
        int z=c*c;
        if((x+y==z)||(y+z==x)||(x+z==y))
        {
            cout<<"right"<<endl;
        }
        else
        {
            cout<<"wrong"<<endl;
        }
    }

}
